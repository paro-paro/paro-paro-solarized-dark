    "tokenColors": [

        // ----------------------------------------------- global
        
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#EEFFFF"
            }
        },
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "foreground": "#4A4A4A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Comments",
            "scope": "comment punctuation.definition.comment, string.quoted.docstring",
            "settings": {
                "foreground": "#4A4A4A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "Strings Escape",
            "scope": "constant.character.escape, text.html constant.character.entity.named",
            "settings": {
                "foreground": "#EEFFFF"
            }
        },
        {
            "name": "Numbers",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#F78C6C"
            }
        },
        {
            "name": "Booleans, null, undefined...",
            "scope": "constant.language",
            "settings": {
                "foreground": "#F78C6C"
            }
        },
        {
            "name": "Parenthesis", // ()
            "scope": "meta.brace.round",
            "settings": {
                "foreground": "#EEFFFF"
            }
        },
        {
            "name": "Variables",
            "scope": "variable, variable.parameter, variable.language, support.constant, meta.definition.variable entity.name.function, meta.function-call.arguments",
            "settings": {
                "foreground": "#EEFFFF"
            }
        },
        {
            "name": "Variables",
            "scope": "support.variable",
            "settings": {
                "foreground": "#FFCB6B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Entities",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "Functions",
            "scope": "entity.name.function, support.function",
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "Functions (definition)",
            "scope": "meta.function entity.name.function",
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "Functions (calls)",
            "scope": "entity.name.function.call",
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "Functions(parameters)",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Classes",
            "scope": "entity.name, entity.name.type.class, support.type, support.class, meta.use",
            "settings": {
                "foreground": "#FFCB6B"
            }
            },
        {
            "name": "Classes (variables)",
            "scope": "variable.object.property, meta.field.declaration entity.name.function",
            "settings": {
                "foreground": "#F07178"
            }
            },
        {
            "name": "Classes (methods)",
            "scope": "meta.definition.method entity.name.function",
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "name": "Types",
            "scope": "support.type, constant.other.key",
            "settings": {
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "Types",
            "scope": "entity.name.type, entity.other.inherited-class, entity.other",
            "settings": {
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "Modules",
            "scope": "support.module, support.node",
            "settings": {
                "foreground": "#F07178",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords", // if, import, export, switch...
            "scope": "keyword, modifier, support.type.object",
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "Keywords", // this, super...
            "scope": [
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#F07178",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords",
            "scope": "keyword.other",
            "settings": {
                "foreground": "#F78C6C"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword.control"
            ],
            "settings": {
                "foreground": "#89DDFF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constant Keywords",
            "scope": "constant.keyword",
            "settings": {
                "foreground": "#89DDFF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Storage", // const, let, var...
            "scope": "storage.type, storage.modifier, storage.control",
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "Template expression",
            "scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "Reset embedded/template expression colors",
            "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
            "settings": {
                "foreground": "#EEFFFF"
            }
        },

