{
  "publisher": "paro-paro",
  "name": "paro-paro-solarized-dark",
  "displayName": "Paro Paro Solarized Dark",
  "type": "module",
  "version": "1.2.2",
  "description": "Visual Studio Code Theme based on Solarized Dark",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/paro-paro/paro-paro-solarized-dark.git"
  },
  "keywords": [
    "vscode",
    "theme",
    "dark",
    "solarized",
    "paro-paro"
  ],
  "categories": [
    "Themes"
  ],
  "icon": "icon/icon.png",
  "engines": {
    "vscode": "^1.76.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "Paro Paro Solarized Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "build": "vsce package --yarn --out ./dist",
    "release": "bumpp && vsce publish --yarn"
  },
  "devDependencies": {
    "@paro-paro/eslint-config": "^0.2.0",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0"
  }
}
